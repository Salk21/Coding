#include <iostream>
using namespace std;

int main()
{
	int width = 30;
	int height = 13;
	bool space;
	int j = 0;
	
	for (int i = 0; i < height; i++)
	{
		space = false;
		if (i % 2 != 0 && i <= 6)
		{
			cout << "";
			j = 0;
			space = true;
		}
		for (j; j < width; j++)
		{
			if (i <= 8 && j <= 6)
			{
				if (space == true && j == 6)
				{
					cout << "*";
				}
				else
				{
					cout << "*";
				}
			}
			else
			{
				cout << "_";
			}
		}
		j = 0;
		cout << endl;
	}
}
